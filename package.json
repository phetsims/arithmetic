{
  "name": "arithmetic",
  "version": "1.1.0-dev.4",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/arithmetic.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "ARITHMETIC",
    "phetLibs": [
      "twixt",
      "vegas"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet",
      "phet-io"
    ],
    "simFeatures": {
      "supportsSound": true,
      "supportedRegionsAndCultures": [
        "africa",
        "africaModest",
        "asia",
        "latinAmerica",
        "oceania",
        "random",
        "usa"
      ],
      "supportsDynamicLocale": true
    },
    "simulation": true,
    "phet-io": {
      "validation": false,
      "wrappers": [
        "phet-io-sim-specific/repos/arithmetic/wrappers/check-answers"
      ]
    },
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "ARITHMETIC/multiply",
      "ARITHMETIC/factor",
      "ARITHMETIC/divide"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
}